<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<VBox spacing="10.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.timerdemo.Controller">
    <children>
        <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="17.0" prefWidth="248.0" text="Press Start.Use Arows to set direction" />
        <Pane prefHeight="250.0" prefWidth="250.0">
            <children>
                <ImageView fx:id="Character" fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true" />
            </children>
        </Pane>
        <Button fx:id="startBtn" mnemonicParsing="false" onAction="#start" text="Start" />
    </children>
</VBox>
